@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{

}


@if (Model != null && Model.PageItem != null)
{

    <div class="contact-item">
        <i class=@Html.Sitecore().Field("Icon_Name").ToString()></i>
        <div class="contact-text">
            <h2>@Html.Sitecore().Field("Field_Name")</h2>
            <p>@Html.Sitecore().Field("Field_Value")</p>
        </div>
    </div>

}
